{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
       "no-var-requires":false,
       "array-type":false,
       "no-unnecessary-initializer":false,
       "no-console": [
        true,
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ]
    },
    "rulesDirectory": []
}